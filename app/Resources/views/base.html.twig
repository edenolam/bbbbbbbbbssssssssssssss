    {# <a target="_blank" href="{{ path('homepage', { '_switch_user': '20000056000025' }) }}">Se connecter en tant que</a> #}


    {% if firstconn is not defined %}
        {% set firstconn = false %}
    {% endif %}

    {% set user_cdg_for_droit = utilisateur_cdg.findByUtilisateur(app.user) %}
    {% set user_droit = utilisateur_droits.getDroitByCdg(user_cdg_for_droit) %}
    {% for value in user_droit %}
        {% if(loop.last) %}
            {% set idDepartement = idDepartement ~ value.cdgDepartement.departement.idDepa %}
        {% else %}
            {% set idDepartement = idDepartement ~ value.cdgDepartement.departement.idDepa ~ ',' %}
        {% endif %}
    {% endfor %}

    {% set currentCampagne = campagne_service.GetCurrentCampagne() %}
