            {% include '@Core/modal_contact_par_defaut_null.html.twig' %}
            {% include '@Core/firstconnection.html.twig' %}
        {% endif %}
        {% if idColl != '' and enqueteExist != '' %}
            {% set consolideApaExist = information_bilansocial_saisie.getCurrentInfoBilanSocial(idColl, enqueteExist.idEnqu) %}
            {% set QuestionnaireExist = collectivite_consolide_service.findQuestionnaire(idColl, enqueteExist.idEnqu) %}
            {% set info_saisie = information_bilansocial_saisie.getCurrentInfoBilanSocial(idColl, enqueteExist) %}
            {% set bilanConso = consolide_service.findByTransmis(idColl, enqueteExist.idEnqu) %}
            {% set InformationgeneraleExist = information_generale_agent.GetInformationGenerale(idColl, enqueteExist, currentCampagne.idCamp) %}
            {% set InformationCollectiviteExist = information_collectivite_agent.GetInformationCollectivite(idColl, enqueteExist, currentCampagne.idCamp) %}


{#            {% set InformationCollectiviteExist = information_collectivite_agent.GetInformationCollectivite(idColl, enqueteExist, currentCampagne.idCamp) %}#}
        {% endif %}


        {% if is_granted("ROLE_CDG") %}
            {% set collectivites = user_collectivite.getCollectiviteByUtilisateurAnalyse(app.user, currentCampagne.idCamp) %}
            {% if collectivites is not empty %}
                {% set blAnalyse = true %}
            {% endif %}
             {% set currentEnquete = current_enquete_departement.getDepartementsWithEnqueteByUtilisateurAndDroitFormStatement(app.user.idUtil) %}
        {% elseif is_granted("ROLE_COLLECTIVITY") %}
            {% if currentCampagne is defined and currentCampagne is not empty and currentCampagne is not null %}
                {% set cdg = user_cdg.findCdgByCollectivite(app.user) %}
                {% set blAnalyse = modele_analyse_service.checkIfShowAnalyseLink(cdg,idColl,currentCampagne) %}
            {% endif %}
            {% set currentEnquete = current_enquete.getEnqueteActive(idColl, currentCampagne.idCamp) %}
            {% set hasCurrentEnqueteParam = currentEnquete is empty ? null :enquete_Collectivite.findHasParamCollectiviteEnqueteLancee(app.user.collectivite, currentEnquete.idEnqu) %}
        {% endif %}
    {% endif %}
    {% if info_saisie != '' %}
        {% if info_saisie.initSource == 'n4ds' and info_saisie.blCons == 0 %}
            {% set url =  path('bilansocialagent_index')  %}
        {% elseif info_saisie.initSource == 'n4ds' and info_saisie.blCons == 1 %}
            {% set url =  path('bilan_conso_edit')  %}
        {% elseif info_saisie.initSource == 'basecarr' and info_saisie.blCons == 0 %}
            {% set url =  path('bilansocialagent_index')  %}
        {% elseif info_saisie.initSource == 'basecarr' and info_saisie.blCons == 1%}
            {% set url =  path('bilan_conso_edit')  %}
        {% elseif info_saisie.initSource == 'bs-vide' %}
            {% set url =  path('bilan_conso_edit')  %}
        {% elseif info_saisie.initSource == 'bs-dgcl' %}
            {% set url =  path('bilan_conso_edit')  %}
        {% elseif info_saisie.initSource == 'manu' and info_saisie.blApa == true  and info_saisie.blCons == false%}
            {% set url =  path('bilansocialagent_index')  %}
        {% elseif info_saisie.initSource == 'manu' and info_saisie.blApa == false and (info_saisie.blCons == true or info_saisie.blCons is null ) %}
             {% set url =  path('bilan_conso_edit')  %}
        {% elseif info_saisie.initSource == 'manu' and info_saisie.blApa == true and info_saisie.blCons == true %}
             {% set url =  path('bilan_conso_edit')  %}
        {% endif %}
    {% endif %}





