{% xlsdocument %}
    {% if isEmpty == true %}
        {% xlssheet %} {% endxlssheet %}
    {% else %}
        {% set campagne = campagne_service.GetCurrentCampagne() %}
        {% set AnneeCamp = '' %}
        {% if campagne is not null %}
            {% set AnneeCamp = nmAnnee is defined ? nmAnnee : campagne.nmAnne %}
        {% endif %}

        {% set subtitleStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                }
                            },
                            alignment: {
                                vertical: 'center',
                                horizontal: 'center',
                                wrapText: true
                            },
                            font: {
                                bold: true
                            } } %}
                            
        {% set subtitleLeftStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                }
                            },
                            font: {
                                bold: true
                            },
                            alignment: {
                                vertical: 'center',
                                wrapText: true,
                            } } %}
        
        {% set borderStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                }
                            },
                            alignment: {
                                vertical: 'center',
                                wrapText: true,
                            } } %}
                            
        {% set labelStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                }
                            },
                            alignment: {
                                indent: 1,
                                vertical: 'center',
                                wrapText: true
                            } } %}
                            
        {% set valueStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                },
                            },
                            alignment: {
                                indent: 1,
                                vertical: 'center',
                                wrapText: true
                            },
                            fill: {
                                fillType: 'solid',
                                startColor: {
                                    rgb: 'D9D9D9'
                                },
                                endColor: {
                                    rgb: 'D9D9D9'
                                }
                            } } %}
                            
        {% set valueBoolStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                },
                            },
                            alignment: {
                                vertical: 'center',
                                horizontal: 'center',
                                wrapText: true
                            },
                            fill: {
                                fillType: 'solid',
                                startColor: {
                                    rgb: 'D9D9D9'
                                },
                                endColor: {
                                    rgb: 'D9D9D9'
                                }
                            } } %}
                            
        {% set totalStyle = {borders: {
                                allBorders: {
                                    borderStyle: 'thin',
                                    color: {
                                        rgb: '000000'
                                    }
                                },
                            },
                            alignment: {
                                indent: 1,
                                vertical: 'center',
                                wrapText: true
                            },
                            fill: {
                                fillType: 'solid',
                                startColor: {
                                    rgb: 'BFBFBF'
                                },
                                endColor: {
                                    rgb: 'BFBFBF'
                                }
                            } } %}
        {% set blHand = false %}
        {% set blRast = false %}
        {% set blGepe = false %}

        {% set blQ22 = false %}
        {% set blQ23 = false %}


        {% for qb22 in QHandiB22 if qb22 == true %}
            {% set blQ22 = true %}
        {% endfor %}

        {% for qb23 in QHandiB23 if qb23 == true %}
            {% set blQ23 = true %}
        {% endfor %}




        {% for enqueteColl in mesEnquetesCollectivites if enqueteColl.blHand %}
            {% set blHand = true %}
        {% endfor %}
        {% for enqueteColl in mesEnquetesCollectivites if enqueteColl.blRast %}
            {% set blRast = true %}
        {% endfor %}

        {% for enqueteColl in mesEnquetesCollectivites if enqueteColl.blGepe %}
            {% set blGepe = true %}
        {% endfor %}

        {% if(blHand == true) %}
            {% xlssheet 'Handitorial' {
                tabColor: 'AD5362',
                columnDimension: {
                    'A': {
                        width: '120'
                    },
                    'B': {
                        width: '16'
                    },
                    'C': {
                        width: '16'
                    },
                    'D': {
                        width: '16'
                    }
                }
                } %}

                {% xlsrow %}
                    {% xlscell {
                        style: {
                            fill: {
                                fillType: 'solid',
                                startColor: {
                                    rgb: 'AD5362'
                                },
                                endColor: {
                                    rgb: 'AD5362'
                                }
                            },
                            alignment: {
                                horizontal: 'center',
                                wrapText: true
                            },
                            font: {
                                size: '18',
                                color: {
                                    rgb: 'FFFFFF'
                                }
                            }
                        }
                    } %}
                        IND 9.1 - L'obligation d'emploi de travailleurs handicapés
                    {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% xlsrow %}
                    {% xlscell {
                        style: borderStyle
                    } %}  
                        Informations générales
                    {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}
                <!-- Informations générales -->
                {% xlsrow %}
                    {% xlscell { style: subtitleStyle } %}Libellés{% endxlscell %}
                    {% xlscell { style: subtitleStyle } %}Réponses{% endxlscell %}
                {% endxlsrow %}
                {% if (is_granted("ROLE_COLLECTIVITY")) %} 
                    {% xlsrow %}
                        {% xlscell { style: borderStyle } %} A1-7 Mail du correspondant Handicap {% endxlscell %}
                        {% xlscell { style: valueStyle } %} 
                                {% if (bscQA17 is not null) %} 
                                    {{ bscQA17.qA17 is defined ? bscQA17.qA17 : "" }}
                                {% endif %} 
                        {% endxlscell %}      
                    {% endxlsrow %}
                {% endif %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A3 - Au 31/12 de l'année {{ AnneeCamp }}, la collectivité était-elle soumise à l'obligation d'emploi de travailleurs handicapés ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} 
                        {% if (is_granted("ROLE_COLLECTIVITY")) %} 
                            {% if (bscQA3 is not null and bscQA3.qA3 == true) %} 
                                Oui
                            {% else %} 
                                Non 
                            {% endif %} 
                        {% endif %} 
                        {% if (is_granted("ROLE_CDG") or (is_granted("ROLE_INFOCENTRE"))) %} 
                            Oui 
                        {% endif %} 
                    {% endxlscell %}      
                {% endxlsrow %}

                <!-- Fin Informations générales -->

                <!-- Inaptitude et reclassement -->

                {% xlsrow %}
                    {% xlscell {
                        style: borderStyle
                    } %}  
                        IND 9.1.1 Inaptitude et reclassement
                    {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% xlsrow %}
                    {% xlscell { style: subtitleStyle} %} Libellés {% endxlscell %}
                    {% xlscell { style: subtitleStyle } %} Réponses {% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-1-1 - Combien d'avis d'inaptitude temporaire ont été délivrés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA511 is defined ? bscHandiInapEtRecla.qA511 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-1-2 - Combien d'avis d'inaptitude définitive à ses fonctions ont été délivrés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA512 is defined ? bscHandiInapEtRecla.qA512 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-1-3 - Combien d'avis d'inaptitude définitive à toutes fonctions ont été délibrés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA513 is defined ? bscHandiInapEtRecla.qA513 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-2-0 - Combien de reclassements ont été demandés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.rA9 is defined ? bscHandiInapEtRecla.rA9 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-2-1 - Combien de reclassements (arrêtés) ont été prononcés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA521 is defined ? bscHandiInapEtRecla.qA521 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-2-1-1 - Combien faisaient suite à une inaptitude liée à un accident du travail ou une maladie professionnelle {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.rA101 is defined ? bscHandiInapEtRecla.rA101 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-2-2 - Combien de retraites pour invalidité ont été prononcés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA522 is defined ? bscHandiInapEtRecla.qA522 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-2-3 - Combien de licenciements pour inaptitude physique ont été prononcés au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA523 is defined ? bscHandiInapEtRecla.qA523 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A6-2 - Combien de mesures d'aménagement de poste ou des conditions de travail (hors avis de reclassement) ont-elles été prononcées au cours de l'année {{ AnneeCamp }} au bénéfice de B.O.E.T.H ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA62 is defined ? bscHandiInapEtRecla.qA62 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A7-2 - Combien de mesures de changement d'affectation (hors avis de reclassement) ont-elles été prononcées au cours de l'année {{ AnneeCamp }} au bénéfice de B.O.E.T.H ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA72 is defined ? bscHandiInapEtRecla.qA72 : 0 }} {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A8 - Des mesures de disponibilités d'office (hors avis de reclassement) ont-elles été prononcées au cours de l'année {{ AnneeCamp }} {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} 
                        {% if (is_granted("ROLE_COLLECTIVITY")) %} 
                            {% if (bscQA8 is not null and bscQA8.qA8 == true) %} 
                                Oui
                            {% else %} 
                                Non 
                            {% endif %} 
                        {% endif %} 
                        {% if (is_granted("ROLE_CDG") or (is_granted("ROLE_INFOCENTRE"))) %} 
                            Oui 
                        {% endif %} 
                    {% endxlscell %}      
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A8-2 - Combien de mesures de disponibilité d'office (hors avis de reclassement) ont-elles été prononcées au cours de l'année {{ AnneeCamp }} au bénéfice de B.O.E.T.H ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %} {{ bscHandiInapEtRecla.qA82 is defined ? bscHandiInapEtRecla.qA82 : 0 }} {% endxlscell %}      
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% xlsrow %}
                    {% xlscell %}{% endxlscell %}
                    {% xlscell { style: subtitleStyle } %}Oui / Non{% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} A5-1  Des agents ont-ils bénéficiés d'un avis d'inaptitude au cours de l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %}

                     {% if blQ22 == true %}
                         Oui
                        {% else %}
                         Non
                        {% endif %}
                    {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% if bscRepo.isPropInAnnee('bscHanditorialAvisInaptitudes',nmAnnee) %}
                    {% xlsrow %}
                        {% xlscell { style: subtitleLeftStyle } %} Avis Inaptitude au cours de l'année {{ AnneeCamp }} {% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                    {% endxlsrow %}
                    {% for value in bscHandiAvisInaps %}
                        {% xlsrow %}
                            {% xlscell { style: labelStyle } %} {{ value.lbInaptitudeBoeth|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.avisInaptitudeH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.avisInaptitudeF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}

                {% if bscRepo.isPropInAnnee('bscHanditorialMesureInaptitudes',nmAnnee) %}
                    {% xlsrow %}
                        {% xlscell { style: subtitleLeftStyle } %} A5-2  Mesure suite à une inaptitude au cours de l'année {{ AnneeCamp }} {% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                    {% endxlsrow %}
                    {% for value in bscHandiMesureInaps %}
                        {% xlsrow %}
                            {% xlscell { style: labelStyle } %} {{ value.lbMesureBoeth|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.mesureInaptitudeH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.mesureInaptitudeF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}

                {% xlsrow %}
                    {% xlscell %}{% endxlscell %}
                    {% xlscell { style: subtitleStyle } %}Oui / Non{% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: borderStyle } %} Des agents ont-ils bénéficiés d'un avis d'inaptitude avant l'année {{ AnneeCamp }} ? {% endxlscell %}
                    {% xlscell { style: valueBoolStyle } %}
                        {% if  blQ23  == true %}
                            Oui
                        {% else %}
                            Non
                        {% endif %}
                        {#{% if (bscHandiAvisInapsAvant is not empty) %}{% if bscHandiAvisInapsAvant == 1 %}Oui{% else %}Non{% endif %}{% else %} Non {% endif %}
                        #}
                        {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% if bscRepo.isPropInAnnee('bscHanditorialAvisInaptitudesAvant',nmAnnee) %}
                    {% xlsrow %}
                        {% xlscell { style: subtitleLeftStyle } %} Avis Inaptitude au cours de l'année {{ AnneeCamp-1 }} {% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                    {% endxlsrow %}
                    {% for value in bscHandiAvisInapsAvant %}
                        {% xlsrow %}
                            {% xlscell { style: labelStyle } %} {{ value.lbInaptitudeAvant|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.avisInaptitudeAvantH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.avisInaptitudeAvantF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}

                {% if bscRepo.isPropInAnnee('bscHanditorialMesureInaptitudesAvant',nmAnnee) %}
                    {% xlsrow %}
                        {% xlscell { style: subtitleLeftStyle } %} A5-2  Mesure suite à une inaptitude au cours de l'année {{ AnneeCamp-1 }} {% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                        {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                    {% endxlsrow %}
                    {% for value in bscHandiMesureInapsAvant %}
                        {% xlsrow %}
                            {% xlscell { style: labelStyle } %} {{ value.lbMesureAvant|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.mesureInaptitudeAvantH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.mesureInaptitudeAvantF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}

                {% xlsrow %}
                    {% xlscell {
                        style: subtitleStyle
                    } %}  
                        IND 9.1.2.4 Ancienneté des agents {{ AnneeCamp }}
                    {% endxlscell %}
                {% endxlsrow %}

                {% xlsrow %}{%endxlsrow%}

                {% xlsrow %}
                    {% xlscell %}{% endxlscell %}
                    {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                    {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: subtitleLeftStyle } %} Moins de 1 an {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.moinsUnAnH is defined ? bscHandiAncienneteAgents.moinsUnAnH : 0 }} {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.moinsUnAnF is defined ? bscHandiAncienneteAgents.moinsUnAnF : 0 }} {% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: subtitleLeftStyle } %} Entre 1 et 3 ans {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreUnEtTroisH is defined ? bscHandiAncienneteAgents.entreUnEtTroisH : 0 }} {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreUnEtTroisF is defined ? bscHandiAncienneteAgents.entreUnEtTroisF : 0 }} {% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: subtitleLeftStyle } %} Entre 4 et 6 ans {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreQuatreEtSixH is defined ? bscHandiAncienneteAgents.entreQuatreEtSixH : 0 }} {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreQuatreEtSixF is defined ? bscHandiAncienneteAgents.entreQuatreEtSixF : 0 }} {% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: subtitleLeftStyle } %} Entre 7 et 12 ans {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreSeptEtDouzeH is defined ? bscHandiAncienneteAgents.entreSeptEtDouzeH : 0 }} {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.entreSeptEtDouzeF is defined ? bscHandiAncienneteAgents.entreSeptEtDouzeF : 0 }} {% endxlscell %}
                {% endxlsrow %}
                {% xlsrow %}
                    {% xlscell { style: subtitleLeftStyle } %} Plus de 12 ans {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.plusDouzeH is defined ? bscHandiAncienneteAgents.plusDouzeH : 0 }} {% endxlscell %}
                    {% xlscell { style: valueStyle } %} {{ bscHandiAncienneteAgents.plusDouzeF is defined ? bscHandiAncienneteAgents.plusDouzeF : 0 }} {% endxlscell %}
                {% endxlsrow %}

                <!-- Fin Inaptitude et reclassement -->

                
                {% if bscRepo.isPropInAnnee('bscHanditorialInaptEtReclaCadreEmplois',nmAnnee) %}
                    <!-- Répartition des agents inaptes ou reclassés selon le cadre d'emploi -->
                    {% xlsrow %}
                        {% xlscell {
                            style: subtitleStyle
                        } %}  
                            Répartition des agents inaptes ou reclassés selon le cadre d'emplois en {{ AnneeCamp }}
                        {% endxlscell %}
                    {% endxlsrow %}

                    {% set filiIncrement = '1' %}

                    {% for value in bscHandiInapEtReclaCadreEmplois %}
                        {% if(value.idFili == filiIncrement) %}
                            {% xlsrow %}{%endxlsrow%}
                            {% xlsrow %}
                                {% xlscell { style: subtitleLeftStyle } %}{{ value.lbFili|upper|raw|raw }}{% endxlscell %}
                                {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                                {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                            {% endxlsrow %}
                            {% set filiIncrement = filiIncrement + 1 %}
                        {% endif %}
                        {% xlsrow %}
                            {% xlscell { style: labelStyle }  %} {{ value.refCadreEmploi|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.cadreEmploiH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.cadreEmploiF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    <!-- Fin Répartition des agents inaptes ou reclassés selon le cadre d'emploi -->

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}
                

                {% if bscRepo.isPropInAnnee('bscHanditorialInaptEtReclaMetiers',nmAnnee) %}
                    <!-- Répartition des agents inaptes ou reclassés selon le métier -->
                    {% xlsrow %}
                        {% xlscell {
                            style: subtitleStyle
                        } %}  
                            Répartition des agents inaptes ou reclassés selon le métier exercé en {{ AnneeCamp }}
                        {% endxlscell %}
                    {% endxlsrow %}

                    {% set familleIncrement = '1' %}

                    {% for value in bscHandiInapEtReclaMetiers %}
                        {% if(value.idFamilleMetier == familleIncrement) %}
                            {% xlsrow %}{%endxlsrow%}
                            {% xlsrow %}
                                {% xlscell { style: subtitleLeftStyle } %}{{ value.lbFamilleMetier|upper|raw|raw }}{% endxlscell %}
                                {% xlscell { style: subtitleStyle } %}Code CNFPT{% endxlscell %}
                                {% xlscell { style: subtitleStyle } %}Homme{% endxlscell %}
                                {% xlscell { style: subtitleStyle } %}Femme{% endxlscell %}
                            {% endxlsrow %}
                            {% set familleIncrement = familleIncrement + 1 %}
                        {% endif %}

                        {% xlsrow %}
                            {% xlscell { style: labelStyle }  %} {{ value.lbMetier|raw }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.cdMetier }} {% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.metierH }}{% endxlscell %}
                            {% xlscell { style: valueStyle } %}{{ value.metierF }}{% endxlscell %}
                        {% endxlsrow %}
                    {% endfor %}

                    <!-- Fin Répartition des agents inaptes ou reclassés selon le métier -->

                    {% xlsrow %}{%endxlsrow%}
                {% endif %}

                <!-- Répartition des agents inaptes selon le temps de travail -->

                {% xlsrow %}
                    {% xlscell {
                        style: subtitleStyle
                    } %}  
                        Répartition des agents inaptes selon le temps de travail
