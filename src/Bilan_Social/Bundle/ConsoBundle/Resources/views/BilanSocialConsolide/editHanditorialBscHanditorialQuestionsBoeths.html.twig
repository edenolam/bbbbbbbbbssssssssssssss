                    notify_move('#b4_1a', 'add', { 'title' : "Des agents ont-ils été titularisé au cours de l'année ?"});
                    notify_move('#bscHanditorialModeSortiesTitulaireTable', 'add', { 'title' : "Quels sont les modes de sorties si titulaires ?"});
                }
