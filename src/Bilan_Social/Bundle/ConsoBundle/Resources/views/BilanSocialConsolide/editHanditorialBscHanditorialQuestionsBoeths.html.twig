                }
            } else {

                $('#bscHanditorialArticlesTable').show();
                $('#bscHanditorialModeSortiesNonTitulaireTable').show();
                if(bool){
                    notify_move('#bscHanditorialArticlesTable', 'add', {'title' : "Sur la base de quels articles de quel alinéa de l'article 3 de la loi n°84-53 ont-ils été nommés ?"});
                    notify_move('#bscHanditorialModeSortiesNonTitulaireTable', 'add', {'title' : "Quels sont les modes de sorties si non titulaires ?"});
                }


            }
        }
        $(document).on('click','.notify_alert', function(){
           scrollTo($(this).data('link'));
        });
        function notify_move(element_moved, action, option){

            $.notify.addStyle('link', {
                html: "<div data-link='"+element_moved+"' class='alert alert-success notify_alert' role='alert'><span data-notify-html /></div>",
                classes: {
                    base: {
                        "background-color": "lightblue",
                        "padding": "5px",
                        "width" : "50%"
                    }
                }
            });

            var str_action = 'modifié';
            var str_title = 'Un(e) tableau/Question ';

            if(action === 'add'){
                str_action = "a été ajouté";
            }else if(action === 'del'){
                str_action = "a été supprimé";
            }

            if(!isEmpty(option.title)){
                str_title = option.title;
            }
            var text =  "<strong>" + str_title + "</strong> " + str_action + " cliquer ici pour y acceder";
            var element_exist = $(document).find('.notify_alert');
            var bool = true;
            $( element_exist ).each(function() {
                if($(this).data('link') == element_moved){
                    bool = false;
                }
            });

            if(bool){

                $.notify(text,  {
                    position: 'bottom left',
                    // default positions
                    elementPosition: 'bottom left',
                    globalPosition: 'bottom left',
                    // default style
                    style: 'link',
                    // default class (string or [string])
                    className: 'alert alert-info notify_alert',
                    showDuration: 700,
                    autoHideDelay: 15000,
                    arrowShow: false,
                    autoHide: true
                });
            }else{
                return false;
            }

        }

