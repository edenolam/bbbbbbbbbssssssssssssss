        function isTitulaire(bool) {
            var val = 0;
            $('.TITU').find('input').each(function() {
                {#var inputValue = $(this).find('input').val();
                val += parseInt(val) + parseInt(inputValue != '' ? inputValue : 0);#}
                var inputValue = $(this).val();
                val = parseInt(val) + parseInt(inputValue != '' ? inputValue : 0);
            });
            if(val == 0) {
                $('#b4_1a').hide();
                $('#bscHanditorialModeSortiesTitulaireTable').hide();
