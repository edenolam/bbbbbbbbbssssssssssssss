<div class="modal fade" id="512" tabindex="-1" role="dialog" aria-labelledby="512" aria-hidden="true">
    <div class="modal-dialog autoSizeModal" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="512">Indicateur 512 de l'année {{ indicateur_precedent.ind_5121.annee }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body" style="background-color: #efefef">
                {% if indicateur_precedent.ind_5122.indicateur is empty %}
                  Aucunes valeurs n'étaient présentes pour l'année {{ indicateur_precedent.ind_5122.annee }}
                {% else %}
                    <table width="100%" id="ind512Table_ind_precedent" class="table table-striped table-hover table-bordered">
                        <thead>
                            <tr entete="1">
                                <th rowspan="2"></th>
                                <th style="text-align: center" colspan="2">Nombre d'agents sur un emploi non permanent présents au 31/12/{{indicateur_precedent.ind_5122.annee}} et ayant participé à au moins une formation en {{indicateur_precedent.ind_5122.annee}}</th>
                            </tr>

                            <tr entete="1">
                                <th>Hommes
                                </th>
                                <th>Femmes
                                </th>
                            </tr>

                        </thead>
                        <tbody>
                        {% set total1 = 0%}
                        {% set total2 = 0%}
                        {% set totalGlo = 0%}

                        {% for tag in indicateur_precedent.ind_5122.indicateur %}
                            {% set total1 = total1 + tag.R_51221 %}
                            {% set total2 = total2 + tag.R_51222 %}
                            {% set totalGlo =  tag.R_51221 + tag.R_51222 %}
                            <tr>
                                <td style="min-width: 400px">
                                    {{ tag.LB_EMPLNONPERM }}
                                </td>
                                <td id="r5121_ind_precedent" style="text-align: center">{{ tag.R_51221 }}</td>
                                <td id="r5122_ind_precedent" style="text-align: center">{{ tag.R_51222 }}</td>
                            </tr>
                        {% endfor %}

                        {% if indicateur_precedent.ind_5121.indicateur|length > 0  %}
                            <tr id="total512">
                                <td style="min-width: 400px"><b>Total tous types </b></td>
                                <td style="text-align: center">{{ total1 }}</td>
                                <td style="text-align: center">{{ total2 }}</td>
                            </tr>
                        {% endif %}
                        </tbody>
                    </table>
                {% endif %}

                {% if indicateur_precedent.ind_5121.indicateur is empty %}
                  Aucunes valeurs n'étaient présentes pour l'année {{ indicateur_precedent.ind_5121.annee }}
                {% else %}

                <table width="100%" id="ind5122Table_ind_precedent" class="table table-striped table-hover table-bordered">
                    <thead>
                        <tr entete="1">
                            <th rowspan="4"></th>
