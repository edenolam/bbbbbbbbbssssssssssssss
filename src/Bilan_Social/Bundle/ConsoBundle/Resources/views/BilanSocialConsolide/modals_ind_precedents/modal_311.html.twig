<div class="modal fade" id="311" tabindex="-1" role="dialog" aria-labelledby="311" aria-hidden="true">
    <div class="modal-dialog autoSizeModal" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="311">Indicateur 311 de l'année {{ indicateur_precedent.annee }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="background-color: #efefef">
                {% if indicateur_precedent.indicateur is empty %}
                  Aucunes valeurs n'étaient présentes pour l'année {{ indicateur_precedent.annee }}
                {% else %}
                    <table width="100%" id="ind311Table_ind_precedent" class="table table-striped table-hover table-bordered">
                        <thead>
                            <tr>
                                <th rowspan="3" style="text-align: center">Fonctionnaire</th>
                                <th colspan="2" style="text-align: center">Montant total des rémunérations annuelles brutes</th>
                                <th colspan="2" style="text-align: center">dont primes et indemnités au titre de l'article 111</th>
                                <th colspan="2" style="text-align: center">dont primes et indemnités (y.c. heures supplémentaires) relevant de l'article 88 (hors remboursement de frais)</th>
                                <th colspan="2" style="text-align: center">dont NBI</th>
                                <th colspan="2" style="text-align: center">dont heures supplémentaires ou complémentaires</th>
                            </tr>
                            <tr>
                                <th colspan="2" style="text-align: center">3.1.1</th>
                                <th colspan="2" style="text-align: center">3.1.1.1</th>
                                <th colspan="2" style="text-align: center">3.1.1.2</th>
                                <th colspan="2" style="text-align: center">3.1.1.3</th>
                                <th colspan="2" style="text-align: center">3.1.1.4</th>
                            </tr>
                            <tr>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                            </tr>
                        </thead>
                        <tbody>
                        {% set total1 = 0%}
                        {% set total2 = 0%}
                        {% set total3 = 0%}
                        {% set total4 = 0%}
                        {% set total5 = 0%}
                        {% set total6 = 0%}
                        {% set total7 = 0%}
                        {% set total8 = 0%}
                        {% set total9 = 0%}
                        {% set total10 = 0%}
                        {% set totalGlo = 0%}

                        {% for tag in indicateur_precedent.indicateur %}
                            {% set total1 = total1 + tag.R_3111 %}
                            {% set total2 = total2 + tag.R_3112 %}
                            {% set total3 = total3 + tag.R_3113 %}
                            {% set total4 = total4 + tag.R_3114 %}
                            {% set total5 = total5 + tag.R_3115 %}
                            {% set total6 = total6 + tag.R_3116 %}
                            {% set total7 = total7 + tag.R_3117 %}
                            {% set total8 = total8 + tag.R_3118 %}
                            {% set total9 = total9 + tag.R_3119 %}
                            {% set total10 = total10 + tag.R_31110 %}
                            {% set totalGlo =  tag.R_3111 + tag.R_3112 + tag.R_3113 + tag.R_3114 + tag.R_3115 + tag.R_3116 %}
                            <tr>
                                <td style="min-width: 400px">
                                    {{ tag.LB_CATE }}
                                </td>
                                <td id="r3111_ind_precedent" style="text-align: center">{{ tag.R_3111 }}</td>
                                <td id="r3112_ind_precedent" style="text-align: center">{{ tag.R_3112 }}</td>
                                <td id="r3113_ind_precedent" style="text-align: center">{{ tag.R_3113 }}</td>
                                <td id="r3114_ind_precedent" style="text-align: center">{{ tag.R_3114 }}</td>
                                <td id="r3115_ind_precedent" style="text-align: center">{{ tag.R_3115 }}</td>
                                <td id="r3116_ind_precedent" style="text-align: center">{{ tag.R_3116 }}</td>
                                <td id="r3117_ind_precedent" style="text-align: center">{{ tag.R_3117 }}</td>
                                <td id="r3118_ind_precedent" style="text-align: center">{{ tag.R_3118 }}</td>
                                <td id="r3119_ind_precedent" style="text-align: center">{{ tag.R_3119 }}</td>
                                <td id="r31110_ind_precedent" style="text-align: center">{{ tag.R_31110 }}</td>
                            </tr>
                        {% endfor %}

                        {% if indicateur_precedent.indicateur|length > 0  %}
                            <tr id="total311_ind_precedent">
                                <td style="min-width: 400px"><b>TOTAL </b></td>
                                <td style="text-align: center">{{ total1 }}</td>
                                <td style="text-align: center">{{ total2 }}</td>
                                <td style="text-align: center">{{ total3 }}</td>
                                <td style="text-align: center">{{ total4 }}</td>
                                <td style="text-align: center">{{ total5 }}</td>
                                <td style="text-align: center">{{ total6 }}</td>
                                <td style="text-align: center">{{ total7 }}</td>
                                <td style="text-align: center">{{ total8 }}</td>
                                <td style="text-align: center">{{ total9 }}</td>
                                <td style="text-align: center">{{ total10 }}</td>
                            </tr>
                        {% endif %}
                        </tbody>
                    </table>
                {% endif %}
            </div>
        </div>
    </div>
</div>