<div class="modal fade" id="121_{{ id_filiere }}" tabindex="-1" role="dialog" aria-labelledby="121_{{ id_filiere }}" aria-hidden="true">
    <div class="modal-dialog autoSizeModal" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="121_modal">Indicateur 121 de l'année {{ indicateur_precedent.annee }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="background-color: #efefef">
                {% if indicateur_precedent.indicateur is empty %}
                  Aucunes valeurs n'étaient présentes pour l'année {{ indicateur_precedent.annee }}
                {% else %}
                
                    <table {#width="100%"#} id="ind121Table_modal" class="table table-striped table-hover table-bordered sticky-header">
                        <thead>
                            <tr entete="1">
                                <th width="10%" rowspan="4"></th>
                                <th colspan="9" style="text-align: center">Type de contrats</th>
                                <th rowspan="2" colspan="10" style="text-align: center"></th>
                            </tr>
                            <tr entete="1">
                                <th colspan="8" style="text-align: center">Agents en CDD</th>
                                <th rowspan="4" style="text-align: center">Agents en CDI</th>
                            </tr>
                            <tr entete="1">
                                <th colspan="7" style="text-align: center">Article 3 de la Loi du 26 janvier 84, modifié par la loi n° 2012-347 du 12 mars 2012-art. 41(V))</th>
                                <th rowspan="3" style="text-align: center">Autres contractuels (articles 38, 38bis, 47,136...)</th>
                                <th class="filiTable" rowspan="3" style="text-align: center">Total</th>
                                <th rowspan="2" colspan="2" style="text-align: center">Tous emplois exerçant à </th>
                                <th rowspan="2" colspan="3" style="text-align: center">Ancienneté dans la collectivité</th>
                                <th rowspan="2" colspan="2" style="text-align: center">Hommes</th>
                                <th rowspan="2" colspan="2" style="text-align: center">Femmes</th>
                            </tr>
                            <tr entete="1">
                                <th style="text-align: center">Article 3-1</th>
                                <th style="text-align: center">Article 3-2</th>
                                <th style="text-align: center">Article 3-3, 1°</th>
                                <th style="text-align: center">Article 3-3, 2°</th>
                                <th style="text-align: center">Article 3-3, 3°</th>
                                <th style="text-align: center">Article 3-3, 4°</th>
                                <th style="text-align: center">Article 3-3, 5°</th>
                            </tr>
                            <tr entete="1">
                                <th style="text-align: center">FILIERES</th>
                                <th style="text-align: center"><span title="Remplaçants" class="buttoninfo"></span></th>
                                <th style="text-align: center"><span title="Affectés sur un poste vacant" class="buttoninfo"></span></th>
                                <th style="text-align: center"><span title="Pas de cadre d'emplois existant" class="buttoninfo"></span> </th>
                                <th style="text-align: center"><span title="Catégorie A selon les fonctions ou pour des besoins de service" class="buttoninfo"></span></th>
                                <th style="text-align: center"><span title="Secrétaire de mairie dans les communes et groupements de communes de moins de 1000 habitants" class="buttoninfo"></span></th>
                                <th style="text-align: center"><span title="Temps non complet des communes et groupements de communes de moins de 1000 hab., lorsque la quotité de temps de travail est inférieure à 50 %" class="buttoninfo"></span></th>
                                <th style="text-align: center"><span title="Communes de moins de 2000 hab. et groupements de communes de moins de 10 000 hab. dont la création ou la suppression dépend de la décision d'une autorité qui s'impose à la collectivité" class="buttoninfo"></span></th>
                                <th style="text-align: center">Temps complet</th>
                                <th style="text-align: center">Temps non complet</th>
                                <th style="text-align: center">Moins de 3 ans</th>
                                <th style="text-align: center">de 3 ans à moins de 6 ans</th>
                                <th style="text-align: center">6 ans et plus</th>
                                <th style="text-align: center">occupant un emploi en CDI</th>
                                <th style="text-align: center">occupant un emploi en CDD</th>
                                <th style="text-align: center">occupant un emploi en CDI</th>
                                <th style="text-align: center">occupant un emploi en CDD</th>
                            </tr>
                        </thead>
                        <tbody id="121tbody_modal">
                            {% for key, filiere in indicateur_precedent.indicateur %}
                                {% if (id_filiere == key) %}
                                    {% for tag in filiere %}
                                        {% set total1 = tag.R_1212 + tag.R_1213 + tag.R_1214 + tag.R_1215 + tag.R_1216 + tag.R_1217 + tag.R_1218 + tag.R_12118 %}
                                        <tr>
                                            <td>
                                                {{ tag.LB_CADREMPL }}

                                            </td>
                                            <td id="r1211" style="text-align: center">{{ tag.R_1211 }}</td>
                                            <td id="r1212" style="text-align: center">{{ tag.R_1212 }}</td>
                                            <td id="r1213" style="text-align: center">{{ tag.R_1213 }}</td>
                                            <td id="r1214" style="text-align: center">{{ tag.R_1214 }}</td>
                                            <td id="r1215" style="text-align: center">{{ tag.R_1215 }}</td>
                                            <td id="r1216" style="text-align: center">{{ tag.R_1216 }}</td>
                                            <td id="r1217" style="text-align: center">{{ tag.R_1217 }}</td>
                                            <td id="r1218" style="text-align: center">{{ tag.R_1218 }}</td>
                                            <td id="r12118" style="text-align: center">{{ tag.R_12118 }}</td>
                                            <td class="filiTable" id="lineTot1" style="text-align: center">{{ total1 }}</td>
                                            <td id="r1219" style="text-align: center">{{ tag.R_1219 }}</td>
                                            <td id="r12110" style="text-align: center">{{ tag.R_12110 }}</td>
                                            <td id="r12111" style="text-align: center">{{ tag.R_12111 }}</td>
                                            <td id="r12112" style="text-align: center">{{ tag.R_12112 }}</td>
                                            <td id="r12113" style="text-align: center">{{ tag.R_12113 }}</td>
                                            <td id="r12114" style="text-align: center">{{ tag.R_12114 }}</td>
                                            <td id="r12115" style="text-align: center">{{ tag.R_12115 }}</td>
                                            <td id="r12116" style="text-align: center">{{ tag.R_12116 }}</td>
                                            <td id="r12117" style="text-align: center">{{ tag.R_12117 }}</td>

                                        </tr>
                                    {% endfor %}
                                {% endif %}
                            {% endfor %}
                        </tbody>
                    </table>
                {% endif %}
            </div>
        </div>
    </div>
</div>