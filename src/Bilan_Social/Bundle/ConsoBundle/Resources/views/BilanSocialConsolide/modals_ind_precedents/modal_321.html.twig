<div class="modal fade" id="321" tabindex="-1" role="dialog" aria-labelledby="321" aria-hidden="true">
    <div class="modal-dialog autoSizeModal" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="321">Indicateur 321 de l'année {{ indicateur_precedent.annee }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="background-color: #efefef">
                {% if indicateur_precedent.indicateur is empty %}
                  Aucunes valeurs n'étaient présentes pour l'année {{ indicateur_precedent.annee }}
                {% else %}
                    <table width="100%" id="ind321Table_ind_precedent" class="table table-striped table-hover table-bordered">
                        <thead>
                            <tr>
                                <th rowspan="3" style="text-align: center">CONTRACTUELS SUR EMPLOIS PERMANENTS</th>
                                <th colspan="2" style="text-align: center">Montant total des rémunérations annuelles brutes</th>
                                <th colspan="2" style="text-align: center">dont primes et indemnités</th>
                                <th colspan="2" style="text-align: center">dont heures supplémentaires ou  complémentaires</th>
                            </tr>
                            <tr>
                                <th colspan="2" style="text-align: center">3.2.1</th>
                                <th colspan="2" style="text-align: center">3.2.1.1</th>
                                <th colspan="2" style="text-align: center">3.2.1.2</th>
                            </tr>
                            <tr>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                                <th style="text-align: center">Hommes</th>
                                <th style="text-align: center">Femmes</th>
                            </tr>
                        </thead>
                        <tbody>
                        {% set total1 = 0%}
                        {% set total2 = 0%}
                        {% set total3 = 0%}
                        {% set total4 = 0%}
                        {% set total5 = 0%}
                        {% set total6 = 0%}
                        {% set totalGlo = 0%}

                        {% for tag in indicateur_precedent.indicateur %}
                            {% set total1 = total1 + tag.R_3211 %}
                            {% set total2 = total2 + tag.R_3212 %}
                            {% set total3 = total3 + tag.R_3213 %}
                            {% set total4 = total4 + tag.R_3214 %}
                            {% set total5 = total5 + tag.R_3215 %}
                            {% set total6 = total6 + tag.R_3216 %}
                            {% set totalGlo =  tag.R_3211 + tag.R_3212 + tag.R_3213 + tag.R_3214 + tag.R_3215 + tag.R_3216 %}
                            <tr>
                                <td style="min-width: 400px">
                                    {{ tag.LB_CATE }}
                                </td>
                                <td id="r3211_ind_precedent" style="text-align: center">{{ tag.R_3211 }}</td>
                                <td id="r3212_ind_precedent" style="text-align: center">{{ tag.R_3212 }}</td>
                                <td id="r3213_ind_precedent" style="text-align: center">{{ tag.R_3213 }}</td>
                                <td id="r3214_ind_precedent" style="text-align: center">{{ tag.R_3214 }}</td>
                                <td id="r3215_ind_precedent" style="text-align: center">{{ tag.R_3215 }}</td>
                                <td id="r3216_ind_precedent" style="text-align: center">{{ tag.R_3216 }}</td>
                            </tr>
                        {% endfor %}

                        {% if indicateur_precedent.indicateur|length > 0  %}
                            <tr id="total321_ind_precedent">
                                <td style="min-width: 400px"><b>TOTAL EMPLOIS FONCTIONNELS</b></td>
                                <td style="text-align: center">{{ total1 }}</td>
                                <td style="text-align: center">{{ total2 }}</td>
                                <td style="text-align: center">{{ total3 }}</td>
                                <td style="text-align: center">{{ total4 }}</td>
                                <td style="text-align: center">{{ total5 }}</td>
                                <td style="text-align: center">{{ total6 }}</td>
                            </tr>
                        {% endif %}
                        </tbody>
                    </table>
                {% endif %}
            </div>
        </div>
    </div>
</div>