{% extends '@Referenciel/layout.html.twig' %}

{% block body %}
    {{parent()}}
    <div class="col-lg-9" >
        <a class="btn button pull-right margin10" href="{{ path('refdomaineprofessionnel_index') }}">Retour à la liste</a>

        <table class="table table-striped"
               <tbody>
                <tr>
                    <th>Code</th>
                    <td>{{ refDomaineProfessionnel.cdDomaineProfessionnel }}</td>
                </tr>
                <tr>
                    <th>Libellé</th>
                    <td>{{ refDomaineProfessionnel.lbDomaineProfessionnel }}</td>
                </tr>
                <tr>
                    <th>Archivé</th>
                    <td>{% if refDomaineProfessionnel.blVali %}Oui{% else %}Non{% endif %}</td>
                </tr>
                {% include '@Referenciel/showRefSharedFieldsLayout.html.twig' with {'ref':refDomaineProfessionnel} %}
            </tbody>
        </table>


        <a class="btn button pull-right margin10" href="{{ path('refdomaineprofessionnel_edit', { 'idDomaineProfessionnel': refDomaineProfessionnel.idDomaineProfessionnel }) }}">Modifier</a>

        {{ form_start(delete_form) }}
        
        {{ form_end(delete_form) }}

    </div>
{% endblock %}

