        <table class="table table-striped"
               <tbody>
                <tr>
                    <th>Code</th>
                    <td>{{ refActeViolencePhysique.cdActviolphys }}</td>
                </tr>
                <tr>
                    <th>Libellé</th>
                    <td>{{ refActeViolencePhysique.lbActviolphys }}</td>
