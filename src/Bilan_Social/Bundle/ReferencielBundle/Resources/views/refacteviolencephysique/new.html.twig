{% extends '@Referenciel/layout.html.twig' %}

{% block body %}
    {{parent()}}

    <div class="col-lg-9" >
        <div class='clearfix margin10'>
            <a class="btn button pull-right" href="{{ path('refacteviolencephysique_index') }}"><span class="glyphicon glyphicon-th-list" aria-hidden="true"></span> Retour à la liste</a>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Actes de violence physique</h3>
            </div>
            <div class="panel-body">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                {{ form_row(form.blExclutotal) }}
                {{ form_row(form.nmOrdre) }}
                {{ form_end(form) }}
                <input class="btn button pull-right margin10" type="submit" value="Créer" />
            </div>
        </div>



    </div>
{% endblock %}


