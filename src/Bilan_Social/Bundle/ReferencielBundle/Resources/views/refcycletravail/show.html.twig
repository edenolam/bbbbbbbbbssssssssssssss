{% extends '@Referenciel/layout.html.twig' %}

{% block body %}
    {{parent()}}
    <div class="col-lg-9" >
        <a class="btn button pull-right margin10" href="{{ path('refcycletravail_index') }}">Retour à la liste</a>

        <table class="table table-striped"
               <tbody>
                <tr>
                    <th>Code</th>
                    <td>{{ refCycleTravail.cdCycltrav }}</td>
                </tr>
                <tr>
                    <th>Libellé</th>
                    <td>{{ refCycleTravail.lbCycltrav }}</td>
                </tr>
                <tr>
                    <th>Groupe</th>
                    <td>{{ refCycleTravail.lbGroupeCycltrav }}</td>
                </tr>
                <tr>
                    <th>Archivé</th>
                    <td>{% if refCycleTravail.blVali %}Oui{% else %}Non{% endif %}</td>
                </tr>
                {% include '@Referenciel/showRefSharedFieldsLayout.html.twig' with {'ref':refCycleTravail} %}
            </tbody>
        </table>


        <a class="btn button pull-right margin10" href="{{ path('refcycletravail_edit', { 'idCycltrav': refCycleTravail.idCycltrav }) }}">Modifier</a>

        {{ form_start(delete_form) }}
        
        {{ form_end(delete_form) }}

    </div>
{% endblock %}

