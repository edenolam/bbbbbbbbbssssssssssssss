<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Bilan_Social\Bundle\ReferencielBundle\Entity\RefTypeCollectivite" table="ref_type_collectivite">
        <cache usage="NONSTRICT_READ_WRITE" region="referentiel_entities" />
        <unique-constraints>
            <unique-constraint columns="CD_TYPECOLL" name="cdTypecoll"/>
        </unique-constraints>
        <id name="idTypeColl" type="integer" column="ID_TYPE_COLL">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="cdTypecoll" type="string" column="CD_TYPECOLL" length="50" nullable="true"/>
        <field name="lbTypeColl" type="string" column="LB_TYPE_COLL" length="150" nullable="true"/>
        <field name="cdUtilcrea" type="string" column="CD_UTILCREA" length="50" nullable="true"/>
        <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
        <field name="cdUtilmodi" type="string" column="CD_UTILMODI" length="50" nullable="true"/>
        <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
        <field name="cdDGCL" type="string" column="CD_DGCL" length="10" nullable="true"/>
        <lifecycle-callbacks>
            <lifecycle-callback type="prePersist" method="setCreatedAtValue" />
            <lifecycle-callback type="preUpdate" method="setUpdateDateValue" />
        </lifecycle-callbacks>
    </entity>
</doctrine-mapping>
