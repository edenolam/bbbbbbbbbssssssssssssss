{% extends '@Apa/layout.html.twig' %}

{% block body %}
    {{ parent() }}
    <h1>Informationgenerales list</h1>

    <table>
        <thead>
            <tr>
                <th>Idenqu</th>
                <th>Idcoll</th>
                <th>Q1</th>
                <th>Q2</th>
                <th>Q3</th>
{#                <th>Q4</th>#}
{#                <th>Q5</th>#}
                <th>Q6</th>
                <th>Q7</th>
                <th>Blheursupp</th>
                <th>Blheurcomp</th>
                <th>Createdat</th>
                <th>Cdutilcrea</th>
                <th>Updatedat</th>
                <th>Cdutilmodi</th>
                <th>Idinfogene</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for informationGenerale in informationGenerales %}
                <tr>
                    <td><a href="{{ path('informationgenerale_show', { 'idInfogene': informationGenerale.idInfogene }) }}">{{ informationGenerale.idEnqu }}</a></td>
                    <td>{{ informationGenerale.idColl }}</td>
                    <td>{% if informationGenerale.q1 %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.q2 %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.q3 %}Yes{% else %}No{% endif %}</td>
{#                    <td>{% if informationGenerale.q4 %}Yes{% else %}No{% endif %}</td>#}
{#                    <td>{% if informationGenerale.q5 %}Yes{% else %}No{% endif %}</td>#}
                    <td>{% if informationGenerale.q6 %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.q7 %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.blHeursupp %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.blHeurcomp %}Yes{% else %}No{% endif %}</td>
                    <td>{% if informationGenerale.createdAt %}{{ informationGenerale.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>{{ informationGenerale.cdUtilcrea }}</td>
                    <td>{% if informationGenerale.updatedAt %}{{ informationGenerale.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>{{ informationGenerale.cdUtilmodi }}</td>
                    <td>{{ informationGenerale.idInfogene }}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('informationgenerale_show', { 'idInfogene': informationGenerale.idInfogene }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('informationgenerale_edit', { 'idInfogene': informationGenerale.idInfogene }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('informationgenerale_new') }}">Create a new informationGenerale</a>
        </li>
    </ul>
{% endblock %}
